<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wielangenoch"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.boppel.jaodernein.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.boppel.jaodernein.CountdownMakerActivity"
            android:label="@string/title_activity_countdown_maker"
            android:parentActivityName="com.boppel.jaodernein.MainActivity" >
        </activity>
        <activity
            android:name="com.boppel.jaodernein.CountdownActivity"
            android:label="@string/title_activity_countdown"
            android:parentActivityName="com.boppel.jaodernein.CountdownListActivity" >
        </activity>
        <activity
            android:name="com.boppel.jaodernein.CountdownListActivity"
            android:label="@string/title_activity_countdown_list"
            android:parentActivityName="com.boppel.jaodernein.MainActivity" >
        </activity>
        <activity
            android:name="com.boppel.tutorial.TutorialActivity"
            android:label="@string/title_activity_tutorial" >
        </activity>
        <activity
            android:name="com.example.wielangenoch.Sasfasf"
            android:label="@string/title_activity_sasfasf" >
        </activity>
    </application>

</manifest>
